<%= form_for(Song.new) do |f| %>
    <label>Song Name:</label><br>
    <%= f.text_field :name %><br>

    <label>Song Genre:</label><br>
    <%= f.number_field :genre_id %><br>

    <label>Song Artist:</label><br>
    <%= f.number_field :artist_id %><br>

    <%= submit_tag "Create Song" %>
<% end %>